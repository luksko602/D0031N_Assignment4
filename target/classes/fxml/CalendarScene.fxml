<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View.CalendarSceneController">
   <children>
      <HBox>
         <children>
            <TextField fx:id="courseCodeField" promptText="Kurskod ">
               <HBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </HBox.margin>
            </TextField>
            <DatePicker fx:id="startDate">
               <HBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </HBox.margin>
            </DatePicker>
            <DatePicker fx:id="endDate">
               <HBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </HBox.margin>
            </DatePicker>
            <Button fx:id="getButton" mnemonicParsing="false" onAction="#getCalendarList" text="Hämta">
               <HBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
      <HBox prefHeight="375.0" prefWidth="600.0">
         <children>
            <VBox alignment="TOP_CENTER" prefHeight="329.0" prefWidth="300.0">
               <children>
                  <Label alignment="CENTER" contentDisplay="CENTER" text="Schema TimeEdit">
                     <VBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </VBox.margin>
                  </Label>
                  <ListView fx:id="calendarList" accessibleRole="CHECK_BOX" onMouseClicked="#displaySelected" prefHeight="346.0" prefWidth="316.0" />
               </children>
            </VBox>
            <VBox alignment="TOP_CENTER" prefHeight="328.0" prefWidth="300.0">
               <children>
                  <Label contentDisplay="CENTER" text="Information">
                     <VBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </VBox.margin>
                  </Label>
                  <HBox>
                     <children>
                        <Label minWidth="80.0" text="Lokal">
                           <HBox.margin>
                              <Insets bottom="3.0" left="3.0" right="3.0" top="6.0" />
                           </HBox.margin>
                        </Label>
                        <TextField fx:id="lokalField" onKeyReleased="#onEditSpecs">
                           <HBox.margin>
                              <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
                           </HBox.margin>
                        </TextField>
                     </children>
                  </HBox>
                  <HBox>
                     <children>
                        <Label minWidth="80.0" text="Lärare">
                           <HBox.margin>
                              <Insets bottom="3.0" left="3.0" right="3.0" top="6.0" />
                           </HBox.margin>
                        </Label>
                        <TextField fx:id="larareField" onKeyReleased="#onEditSpecs">
                           <HBox.margin>
                              <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
                           </HBox.margin>
                        </TextField>
                     </children>
                  </HBox>
                  <HBox>
                     <children>
                        <Label minWidth="80.0" text="Aktivitet">
                           <HBox.margin>
                              <Insets bottom="3.0" left="3.0" right="3.0" top="6.0" />
                           </HBox.margin>
                        </Label>
                        <TextField fx:id="aktivitetField" onKeyReleased="#onEditSpecs">
                           <HBox.margin>
                              <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
                           </HBox.margin>
                        </TextField>
                     </children>
                  </HBox>
                  <HBox>
                     <children>
                        <Label minWidth="80.0" text="Kurs">
                           <HBox.margin>
                              <Insets bottom="3.0" left="3.0" right="3.0" top="6.0" />
                           </HBox.margin>
                        </Label>
                        <TextField fx:id="kursField" onKeyReleased="#onEditSpecs">
                           <HBox.margin>
                              <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
                           </HBox.margin>
                        </TextField>
                     </children>
                  </HBox>
                  <HBox>
                     <children>
                        <Label minWidth="80.0" text="Campus">
                           <HBox.margin>
                              <Insets bottom="3.0" left="3.0" right="3.0" top="6.0" />
                           </HBox.margin>
                        </Label>
                        <TextField fx:id="campusField" onKeyReleased="#onEditSpecs">
                           <HBox.margin>
                              <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
                           </HBox.margin>
                        </TextField>
                     </children>
                  </HBox>
                  <HBox>
                     <children>
                        <Label minWidth="80.0" text="Text">
                           <HBox.margin>
                              <Insets bottom="3.0" left="3.0" right="3.0" top="6.0" />
                           </HBox.margin>
                        </Label>
                        <TextField fx:id="textField1" onKeyReleased="#onEditSpecs">
                           <HBox.margin>
                              <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
                           </HBox.margin>
                        </TextField>
                     </children>
                  </HBox>
                  <HBox>
                     <children>
                        <Label minWidth="80.0" text="Text">
                           <HBox.margin>
                              <Insets bottom="3.0" left="3.0" right="3.0" top="6.0" />
                           </HBox.margin>
                        </Label>
                        <TextField fx:id="textField2" onKeyReleased="#onEditSpecs">
                           <HBox.margin>
                              <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
                           </HBox.margin>
                        </TextField>
                     </children>
                  </HBox>
                  <HBox>
                     <children>
                        <Label minWidth="80.0" text="Syfte">
                           <HBox.margin>
                              <Insets bottom="3.0" left="3.0" right="3.0" top="6.0" />
                           </HBox.margin>
                        </Label>
                        <TextField fx:id="syfteField" editable="false">
                           <HBox.margin>
                              <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
                           </HBox.margin>
                        </TextField>
                     </children>
                  </HBox>
                  <HBox>
                     <children>
                        <Label minWidth="80.0" text="Kurs/Program">
                           <HBox.margin>
                              <Insets bottom="3.0" left="3.0" right="3.0" top="6.0" />
                           </HBox.margin>
                        </Label>
                        <TextField fx:id="kursProgramField" editable="false">
                           <HBox.margin>
                              <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
                           </HBox.margin>
                        </TextField>
                     </children>
                  </HBox>
                  <HBox>
                     <children>
                        <Label minWidth="80.0" text="Kund">
                           <HBox.margin>
                              <Insets bottom="3.0" left="3.0" right="3.0" top="6.0" />
                           </HBox.margin>
                        </Label>
                        <TextField fx:id="kundField" editable="false">
                           <HBox.margin>
                              <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
                           </HBox.margin>
                        </TextField>
                     </children>
                  </HBox>
                  <HBox>
                     <children>
                        <Label minWidth="80.0" text="Utrustning">
                           <HBox.margin>
                              <Insets bottom="3.0" left="3.0" right="3.0" top="6.0" />
                           </HBox.margin>
                        </Label>
                        <TextField fx:id="utrustningField" editable="false">
                           <HBox.margin>
                              <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
                           </HBox.margin>
                        </TextField>
                     </children>
                  </HBox>
               </children>
            </VBox>
         </children>
      </HBox>
      <HBox>
         <children>
            <Button fx:id="backButton" mnemonicParsing="false" onAction="#backToStart" text="Tillbaka">
               <HBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </HBox.margin>
            </Button>
            <Button fx:id="saveButton" mnemonicParsing="false" onAction="#saveEvent" text="Spara">
               <HBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
   </children>
</VBox>
